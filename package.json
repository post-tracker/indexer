{
  "name": "indexer",
  "version": "1.1.0",
  "description": "",
  "main": "dist/indexer.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node dist/indexer.js",
    "build": "babel src -d dist && node dist/setup.js",
    "watch": "babel --watch src -d dist",
    "pretest": "eslint src/**/*.js"
  },
  "engines": {
    "node": "^8.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://kokarn@github.com/post-tracker/indexer.git"
  },
  "author": "Kokarn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/post-tracker/indexer/issues"
  },
  "homepage": "https://github.com/post-tracker/indexer#readme",
  "dependencies": {
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "chalk": "^2.0.1",
    "cheerio": "^1.0.0-rc.1",
    "feedme": "^1.0.0",
    "got": "^6.7.1",
    "html-entities": "^1.2.1",
    "image-type": "^3.0.0",
    "moment": "^2.18.1",
    "mz": "^2.6.0",
    "p-finally": "^1.0.0",
    "sha1": "^1.1.1",
    "twitter": "^1.7.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "eslint": "^3.13.1",
    "eslint-config-kokarn": "^2.0.0"
  },
  "babel": {
    "plugins": [
      "transform-async-to-generator",
      "transform-object-rest-spread"
    ]
  },
  "eslintConfig": {
    "extends": "kokarn/nodejs",
    "parserOptions": {
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      }
    },
    "rules": {
      "node/no-unsupported-features": [
        "error",
        {
          "ignores": [
            "asyncAwait"
          ]
        }
      ],
      "prefer-destructuring": [
        "off"
      ]
    }
  }
}
